{
  "npmScope": "atlascode",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "gnosis-core": {
      "tags": []
    },
    "gnosis-core-e2e": {
      "tags": [],
      "implicitDependencies": ["gnosis-core"]
    },
    "helpers": {
      "tags": []
    },
    "olympus-core": {
      "tags": []
    },
    "olympus-core-e2e": {
      "tags": [],
      "implicitDependencies": ["olympus-core"]
    },
    "react-animations": {
      "tags": []
    },
    "react-components": {
      "tags": []
    },
    "react-hooks": {
      "tags": []
    },
    "react-layouts": {
      "tags": []
    },
    "react-lotties": {
      "tags": []
    },
    "react-pages": {
      "tags": []
    },
    "react-sections": {
      "tags": []
    },
    "react-utility": {
      "tags": []
    }
  }
}
