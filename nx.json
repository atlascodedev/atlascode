{
  "npmScope": "atlascode",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "helpers-assertions": {
      "tags": []
    },
    "helpers-browser": {
      "tags": []
    },
    "helpers-node": {
      "tags": []
    },
    "hooks": {
      "tags": []
    },
    "olympus-core": {
      "tags": []
    },
    "olympus-core-e2e": {
      "tags": [],
      "implicitDependencies": ["olympus-core"]
    },
    "ui-animations": {
      "tags": []
    },
    "ui-components": {
      "tags": []
    },
    "ui-layouts": {
      "tags": []
    },
    "ui-lotties": {
      "tags": []
    },
    "ui-pages": {
      "tags": []
    }
  }
}
