{
  "npmScope": "atlascode",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "core": {
      "tags": []
    },
    "gnosis-core": {
      "tags": []
    },
    "gnosis-core-e2e": {
      "tags": [],
      "implicitDependencies": ["gnosis-core"]
    },
    "olympus-core": {
      "tags": []
    },
    "olympus-core-e2e": {
      "tags": [],
      "implicitDependencies": ["olympus-core"]
    },
    "react": {
      "tags": []
    },
    "react-web-hooks": {
      "tags": []
    },
    "react-web-sliders": {
      "tags": []
    },
    "smooth-scrollbar-react-wrapper": {
      "tags": []
    }
  }
}
