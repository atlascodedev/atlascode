{
  "version": 2,
  "projects": {
    "olympus-core": {
      "root": "apps/olympus/core",
      "sourceRoot": "apps/olympus/core",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/olympus/core",
            "outputPath": "dist/apps/olympus/core"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "olympus-core:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "olympus-core:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "olympus-core:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/olympus/core"],
          "options": {
            "jestConfig": "apps/olympus/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/olympus/core/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "olympus-core-e2e": {
      "root": "apps/olympus/core-e2e",
      "sourceRoot": "apps/olympus/core-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/olympus/core-e2e/cypress.json",
            "tsConfig": "apps/olympus/core-e2e/tsconfig.e2e.json",
            "devServerTarget": "olympus-core:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "olympus-core:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/olympus/core-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "react-data-access": {
      "root": "libs/react/data-access",
      "sourceRoot": "libs/react/data-access/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react/data-access",
            "tsConfig": "libs/react/data-access/tsconfig.lib.json",
            "project": "libs/react/data-access/package.json",
            "entryFile": "libs/react/data-access/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/react/data-access/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react/data-access/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
