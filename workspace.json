{
  "version": 2,
  "projects": {
    "helpers-assertions": {
      "root": "libs/helpers/assertions",
      "sourceRoot": "libs/helpers/assertions/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/helpers/assertions",
            "tsConfig": "libs/helpers/assertions/tsconfig.lib.json",
            "packageJson": "libs/helpers/assertions/package.json",
            "main": "libs/helpers/assertions/src/index.ts",
            "assets": ["libs/helpers/assertions/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/helpers/assertions/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/helpers/assertions"],
          "options": {
            "jestConfig": "libs/helpers/assertions/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
