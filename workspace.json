{
  "version": 2,
  "projects": {
    "gnosis-core": {
      "root": "apps/gnosis/core",
      "sourceRoot": "apps/gnosis/core",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/gnosis/core",
            "outputPath": "dist/apps/gnosis/core"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "gnosis-core:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "gnosis-core:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "gnosis-core:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/gnosis/core"],
          "options": {
            "jestConfig": "apps/gnosis/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gnosis/core/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "gnosis-core-e2e": {
      "root": "apps/gnosis/core-e2e",
      "sourceRoot": "apps/gnosis/core-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gnosis/core-e2e/cypress.json",
            "tsConfig": "apps/gnosis/core-e2e/tsconfig.e2e.json",
            "devServerTarget": "gnosis-core:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gnosis-core:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gnosis/core-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "helpers": {
      "root": "libs/helpers",
      "sourceRoot": "libs/helpers/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/helpers",
            "tsConfig": "libs/helpers/tsconfig.lib.json",
            "packageJson": "libs/helpers/package.json",
            "main": "libs/helpers/src/index.ts",
            "assets": ["libs/helpers/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/helpers/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/helpers"],
          "options": {
            "jestConfig": "libs/helpers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "olympus-core": {
      "root": "apps/olympus/core",
      "sourceRoot": "apps/olympus/core",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/olympus/core",
            "outputPath": "dist/apps/olympus/core"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "olympus-core:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "olympus-core:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "olympus-core:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/olympus/core"],
          "options": {
            "jestConfig": "apps/olympus/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/olympus/core/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "olympus-core-e2e": {
      "root": "apps/olympus/core-e2e",
      "sourceRoot": "apps/olympus/core-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/olympus/core-e2e/cypress.json",
            "tsConfig": "apps/olympus/core-e2e/tsconfig.e2e.json",
            "devServerTarget": "olympus-core:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "olympus-core:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/olympus/core-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "react": {
      "root": "libs/react",
      "sourceRoot": "libs/react/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react",
            "tsConfig": "libs/react/tsconfig.lib.json",
            "project": "libs/react/package.json",
            "entryFile": "libs/react/src/index.ts",
            "external": [
              "react/jsx-runtime",
              "@emotion/styled/base",
              "react-is"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/react/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react"],
          "options": {
            "jestConfig": "libs/react/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react/.storybook"
            },
            "staticDir": ["libs/react/src/assets"]
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react",
            "config": {
              "configFolder": "libs/react/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "react-web-hooks": {
      "root": "libs/react-web-hooks",
      "sourceRoot": "libs/react-web-hooks/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react-web-hooks",
            "tsConfig": "libs/react-web-hooks/tsconfig.lib.json",
            "project": "libs/react-web-hooks/package.json",
            "entryFile": "libs/react-web-hooks/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/react-web-hooks/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-web-hooks/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-web-hooks"],
          "options": {
            "jestConfig": "libs/react-web-hooks/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react-web-hooks/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react-web-hooks",
            "config": {
              "configFolder": "libs/react-web-hooks/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "react-web-ui": {
      "root": "libs/react-web-ui",
      "sourceRoot": "libs/react-web-ui/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/react-web-ui",
            "tsConfig": "libs/react-web-ui/tsconfig.lib.json",
            "project": "libs/react-web-ui/package.json",
            "entryFile": "libs/react-web-ui/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/react-web-ui/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-web-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-web-ui"],
          "options": {
            "jestConfig": "libs/react-web-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/react-web-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/react-web-ui",
            "config": {
              "configFolder": "libs/react-web-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      },
      "component": {
        "style": "@emotion/styled",
        "pascalCaseFiles": "true",
        "export": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "olympus-core"
}
